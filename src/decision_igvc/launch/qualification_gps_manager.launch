<launch>

    <!-- Launch GPS Manager -->
    <node name="gps_manager" pkg="decision_igvc" type="gps_manager" output="screen">
        <rosparam param="at_goal_tolerance">0.5</rosparam>
        <rosparam param="waypoints">
        [
            49.2626385998, -123.248480978,
            49.2624521244, -123.248354995
        
        ]
        </rosparam>
        <rosparam param="base_frame">base_link</rosparam>
        <rosparam param="global_frame">map</rosparam>
    </node>

</launch>
